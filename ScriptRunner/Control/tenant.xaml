<UserControl x:Class="ScriptRunner.Control.tenant"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="450" Background="BlanchedAlmond">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="59*"/>
            <ColumnDefinition Width="11*"/>
        </Grid.ColumnDefinitions>
        <Label Name="tenantlabel" HorizontalAlignment="Center" Height="30" Margin="0,0,0,0" VerticalAlignment="center" Width="275" Grid.Column="0" Grid.Row="0"/>
        <CheckBox Name="tenantck" Grid.Column="1" HorizontalAlignment="Center" Height="14" Margin="0,0,0,0" VerticalAlignment="Center" Width="16" Grid.Row="0" />
    </Grid>
</UserControl>
